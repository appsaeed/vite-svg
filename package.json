{
  "name": "vite-svg",
  "version": "1.0.3",
  "description": "Vite SVG plugin: effortless SVG import with support for various formats JSON, raw string, object, etc.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "rm": "rm -rf ./dist",
    "upload": "tsc && npm publish  --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appsaeed/vite-svg.git"
  },
  "keywords": [
    "vite-svg",
    "vite svg import",
    "svg to json vite",
    "svg raw string",
    "svg loader vite",
    "flexible svg import vite",
    "appsaeed"
  ],
  "author": "appsaeed",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/appsaeed/vite-svg.git/issues"
  },
  "homepage": "https://github.com/appsaeed/vite-svg.git#readme",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "default": "./dist/*.js",
      "require": "./dist/*.js"
    }
  },
  "devDependencies": {
    "@types/node": "^20.12.7"
  }
}
